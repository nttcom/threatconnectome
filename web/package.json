  "scripts": {
    "build": "vite build",
    "check": "npm run check:eslint && npm run check:prettier && npm run check:tsc",
    "check:eslint": "eslint --max-warnings=0 './src/**/*.{js,jsx,ts,tsx}'",
    "check:prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "check:tsc": "tsc --noEmit",
    "dev": "vite dev",
    "fix": "npm run format && npm run lint",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "vitest --run",
    "test:verbose": "vitest --run --reporter=verbose",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi-ts": "openapi-ts",
    "openapi:fetch": "curl http://localhost/api/internal-api/openapi.json -o ./openapi.json",
    "openapi:update": "npm run openapi:fetch && npm run openapi-ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
